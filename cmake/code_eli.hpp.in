# include <qd/dd_real.h>
# include <qd/qd_real.h>
# include <qd/fpu.h>

// add c++11 functions
dd_real cbrt(const dd_real &a)
{
  dd_real n(1./3.);
  return pow(a, n);
}
qd_real cbrt(const qd_real &a)
{
  qd_real n(1./3.);
  return pow(a, n);
}

namespace std
{
  using ::cbrt;
}

